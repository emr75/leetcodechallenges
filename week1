class Solution:
    def twoSum(self, numbers: List[int], target: int) -> List[int]:
            low = 0
            high = len(numbers) - 1
            
            while low < high:
                result = numbers[low] + numbers[high]
                if result > target:
                    high -= 1
                elif result < target:
                    low += 1
                else:
                    return [low + 1, high + 1]


